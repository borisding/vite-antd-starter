{
  "name": "vite-react-antd-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode production",
    "preview": "vite preview",
    "lint:all": "npm-run-all lint:code lint:style",
